    if session.user
      script
        MB.user = new MB.models.User({
          id: '#{session.user.id}'
        });
        MB.isLoggedIn = true;
        MB.user.fetch({
          success: function() {
            MB.user.get('buckets').fetch({
              success: function() {
                MB.router = new MB.Router();
                Backbone.history.start();
              }
            });
          }
        });
    else
      script
        (function() {
          MB.user = new MB.models.User({});
          MB.isLoggedIn = false;
          _sync = Backbone.sync;
          Backbone.sync = function(method, model, options) {
            if (MB.isLoggedIn) {
              _sync(method, model, options);
            }
          };
          MB.router = new MB.Router();
          Backbone.history.start();
        })();

    script
